<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>DF_GetMonthRange</name>
        <uuid>_a-0000dcdc-981d-8000-183f-01ef9001ef90_5941</uuid>
        <description>Displays month-year for a certain interval of months. Pass 1 as the startIndex to return the current month.</description>
        <parentUuid>1c9d8eb9-f682-4add-a209-b09af0151627</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!range: ri!startIndex + enumerate(1 + ri!endIndex - ri!startIndex),
  local!monthDates: apply(
    eomonth(today(), _),
    local!range
  ),
  apply(
    #&quot;_a-0000dcdc-981d-8000-183f-01ef9001ef90_5940&quot;,
    local!monthDates
  )
)</definition>
        <namedTypedValue>
            <name>startIndex</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>endIndex</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
