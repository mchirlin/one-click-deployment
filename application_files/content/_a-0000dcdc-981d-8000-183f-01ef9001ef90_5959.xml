            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>load(
  local!record: #&quot;b86ddab8-a526-492a-a943-f625c5514890_GETONERECORD&quot;(ri!id),
  #&quot;SYSTEM_SYSRULES_dashboardLayout&quot;(
